{
  "name": "bundlegento",
  "private": true,
  "version": "0.1.0",
  "description": "Effective JavaScript bundling for Magento 2 ðŸ“¦",
  "main": "dist/index.js",
  "bin": {
    "bundlegento": "bin/bundlegento"
  },
  "scripts": {
    "build": "tsc",
    "format": "prettier --write $(head -1 prettier-globs)",
    "format:check": "prettier --list-different $(head -1 prettier-globs)",

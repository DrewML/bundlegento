    "url": "https://github.com/magento/bundlegento/issues"
  },
  "homepage": "https://github.com/magento/bundlegento#readme",
  "devDependencies": {
    "@types/chalk": "^2.2.0",
    "@types/cosmiconfig": "^5.0.3",
    "@types/meow": "^5.0.0",
    "@types/puppeteer": "^1.12.1",
    "finalhandler": "^1.1.1",
    "glob": "^7.1.3",
    "jest": "^24.5.0",
    "prettier": "^1.17.0",
    "serve-static": "^1.13.2",
    "ts-jest": "^24.0.0",
    "typescript": "^3.3.3333"
  },
